{
  "name": "generator-air",
  "version": "0.4.6",
  "description": "轻量的前端脚手架 ——— generator-air",
  "scripts": {
    "lint": "eslint generators/** model/**",
    "lint-fix": "eslint --fix generators/** model/**"
  },
  "files": [
    "generators",
    "model"
  ],
  "author": "Sherry",
  "license": "MIT",
  "repository": "https://git.code.tencent.com/FAJG-FE/generator-air.git",
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "shelljs": "^0.8.3",
    "yeoman-generator": "^4.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  }
